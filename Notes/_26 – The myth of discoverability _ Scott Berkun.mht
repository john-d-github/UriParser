.gcard {
	position: absolute;
	z-index: 20;
	-moz-box-shadow: 0 0 8px rgba(0,0,0,.4);
	-webkit-box-shadow: 0 0 8px rgba(0,0,0,.4);
	box-shadow: 0 0 8px rgba(0,0,0,.4);
	max-width: 400px;
	margin: 0;
}

	.gcard .grav-inner {
		zoom: 1;
